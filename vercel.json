{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "JWT_SECRET": "@jwt-secret",
    "MONGODB_URI": "@mongodb-uri-prod"
  }
}
